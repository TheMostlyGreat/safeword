{
  "name": "@safeword/quality",
  "version": "0.1.0",
  "description": "Multi-language code quality configs and runners for Safeword",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./eslint": {
      "import": "./dist/eslint/index.js",
      "types": "./dist/eslint/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TheMostlyGreat/safeword.git",
    "directory": "packages/quality"
  },
  "homepage": "https://github.com/TheMostlyGreat/safeword/tree/main/packages/quality",
  "bugs": {
    "url": "https://github.com/TheMostlyGreat/safeword/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "eslint-plugin-safeword": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "keywords": [
    "eslint",
    "quality",
    "safeword",
    "llm",
    "ai-coding",
    "claude-code",
    "typescript"
  ],
  "author": "Alex Redmon",
  "license": "MIT"
}
